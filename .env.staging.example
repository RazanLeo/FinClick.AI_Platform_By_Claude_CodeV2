# FinClick.AI Staging Environment Variables
# Copy this file to .env.staging and update the values

# ==============================================
# APPLICATION CONFIGURATION
# ==============================================
NODE_ENV=staging
APP_VERSION=1.0.0-staging
APP_NAME=FinClick.AI Staging
APP_URL=https://staging.finclick.ai
API_URL=https://staging-api.finclick.ai

# ==============================================
# DATABASE CONFIGURATION
# ==============================================

# PostgreSQL Configuration
POSTGRES_HOST=postgres
POSTGRES_PORT=5432
POSTGRES_DB=finclick_staging
POSTGRES_USER=finclick_staging
POSTGRES_PASSWORD=staging_password_123

# MongoDB Configuration
MONGO_HOST=mongo
MONGO_PORT=27017
MONGO_DB=finclick_staging
MONGO_ROOT_USER=admin
MONGO_ROOT_PASSWORD=staging_mongo_123

# Redis Configuration
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_PASSWORD=staging_redis_123
REDIS_DB=0

# ==============================================
# MESSAGE QUEUE CONFIGURATION
# ==============================================
RABBITMQ_HOST=rabbitmq
RABBITMQ_PORT=5672
RABBITMQ_USER=guest
RABBITMQ_PASSWORD=guest
RABBITMQ_VHOST=/
RABBITMQ_URL=amqp://guest:guest@rabbitmq:5672/

# ==============================================
# SECURITY CONFIGURATION
# ==============================================

# JWT Configuration
JWT_SECRET_KEY=staging_jwt_secret_key_for_development_only_not_secure
JWT_EXPIRES_IN=24h
JWT_REFRESH_EXPIRES_IN=7d

# Encryption Keys
ENCRYPTION_KEY=staging_32_char_encryption_key1
HASH_SALT_ROUNDS=10

# Session Configuration
SESSION_SECRET=staging_session_secret_key
SESSION_TIMEOUT=1800

# ==============================================
# AI SERVICE CONFIGURATION (Test Keys)
# ==============================================

# OpenAI Configuration (Test)
OPENAI_API_KEY=sk-test-openai-key-for-staging
OPENAI_MODEL=gpt-3.5-turbo
OPENAI_MAX_TOKENS=2000
OPENAI_TEMPERATURE=0.7

# Google Gemini Configuration (Test)
GEMINI_API_KEY=test-gemini-api-key

# Anthropic Claude Configuration (Test)
ANTHROPIC_API_KEY=test-anthropic-api-key

# ==============================================
# FINANCIAL DATA PROVIDERS (Test Keys)
# ==============================================

# Financial Modeling Prep (Demo)
FMP_API_KEY=demo_fmp_api_key
FMP_BASE_URL=https://financialmodelingprep.com/api/v3

# Alpha Vantage (Demo)
ALPHA_VANTAGE_API_KEY=demo_alpha_vantage_key

# Quandl (Test)
QUANDL_API_KEY=test_quandl_api_key

# ==============================================
# PAYMENT PROCESSING (Test Mode)
# ==============================================

# Stripe Configuration (Test)
STRIPE_PUBLISHABLE_KEY=pk_test_staging_stripe_key
STRIPE_SECRET_KEY=sk_test_staging_stripe_secret
STRIPE_WEBHOOK_SECRET=whsec_test_staging_webhook

# PayPal Configuration (Sandbox)
PAYPAL_MODE=sandbox
PAYPAL_CLIENT_ID=sandbox_paypal_client_id
PAYPAL_CLIENT_SECRET=sandbox_paypal_secret

# ==============================================
# EMAIL CONFIGURATION (Test)
# ==============================================

# SMTP Configuration (Test)
SMTP_HOST=mailhog
SMTP_PORT=1025
SMTP_SECURE=false
SMTP_USERNAME=
SMTP_PASSWORD=

# Email Templates
FROM_EMAIL=staging@finclick.ai
FROM_NAME=FinClick.AI Staging
SUPPORT_EMAIL=staging-support@finclick.ai

# ==============================================
# AWS CONFIGURATION (Staging)
# ==============================================

# AWS General
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=staging_aws_access_key
AWS_SECRET_ACCESS_KEY=staging_aws_secret_key

# S3 Configuration
AWS_S3_BUCKET=finclick-staging-files
AWS_S3_REGION=us-east-1
AWS_BACKUP_BUCKET=finclick-staging-backups

# ==============================================
# LOGGING AND MONITORING
# ==============================================

# Log Configuration
LOG_LEVEL=debug
LOG_FORMAT=json
LOG_MAX_FILES=5
LOG_MAX_SIZE=5m

# Sentry Configuration (Staging)
SENTRY_DSN=https://staging-sentry-dsn@sentry.io/project
SENTRY_ENVIRONMENT=staging
SENTRY_RELEASE=1.0.0-staging

# ==============================================
# GRAFANA AND PROMETHEUS
# ==============================================

# Grafana Configuration
GRAFANA_ADMIN_PASSWORD=staging123
GRAFANA_SECRET_KEY=staging_grafana_secret_key

# Prometheus Configuration
PROMETHEUS_RETENTION_TIME=15d
PROMETHEUS_RETENTION_SIZE=5GB

# ==============================================
# ELASTICSEARCH CONFIGURATION
# ==============================================
ELASTIC_USERNAME=elastic
ELASTIC_PASSWORD=staging_elastic_password
ELASTICSEARCH_URL=http://elasticsearch:9200

# ==============================================
# RATE LIMITING (Relaxed)
# ==============================================
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=1000
RATE_LIMIT_SKIP_FAILED_REQUESTS=true

# ==============================================
# CORS CONFIGURATION
# ==============================================
CORS_ORIGIN=https://staging.finclick.ai,http://localhost:3000
CORS_CREDENTIALS=true

# ==============================================
# FILE UPLOAD CONFIGURATION
# ==============================================
MAX_FILE_SIZE=10485760
ALLOWED_FILE_TYPES=pdf,doc,docx,xls,xlsx,csv,txt,png,jpg,jpeg
UPLOAD_PATH=/app/uploads

# ==============================================
# CACHE CONFIGURATION
# ==============================================
CACHE_TTL=1800
CACHE_PREFIX=staging:finclick:
CACHE_MAX_KEYS=5000

# ==============================================
# NOTIFICATION CONFIGURATION
# ==============================================

# Slack Notifications (Staging Channel)
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/staging/webhook
SLACK_CHANNEL=#staging-alerts

# SMS Configuration (Test)
TWILIO_ACCOUNT_SID=staging_twilio_sid
TWILIO_AUTH_TOKEN=staging_twilio_token
TWILIO_PHONE_NUMBER=+1234567890

# ==============================================
# ANALYTICS CONFIGURATION
# ==============================================

# Google Analytics (Staging Property)
GA_TRACKING_ID=UA-STAGING-1

# Mixpanel (Staging Project)
MIXPANEL_TOKEN=staging_mixpanel_token

# ==============================================
# BACKUP CONFIGURATION
# ==============================================
BACKUP_SCHEDULE=0 3 * * *
BACKUP_RETENTION_DAYS=7
BACKUP_ENCRYPTION_KEY=staging_backup_encryption_key

# ==============================================
# PERFORMANCE CONFIGURATION
# ==============================================
NODE_OPTIONS=--max-old-space-size=1024
UV_THREADPOOL_SIZE=64

# ==============================================
# FEATURE FLAGS (More Permissive)
# ==============================================
FEATURE_AI_RECOMMENDATIONS=true
FEATURE_ADVANCED_ANALYTICS=true
FEATURE_REAL_TIME_UPDATES=true
FEATURE_BETA_FEATURES=true
FEATURE_DEBUG_MODE=true

# ==============================================
# THIRD-PARTY INTEGRATIONS (Test)
# ==============================================

# Supabase (Staging)
SUPABASE_URL=https://staging-project.supabase.co
SUPABASE_ANON_KEY=staging_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=staging_supabase_service_key

# Firebase (Staging)
FIREBASE_PROJECT_ID=finclick-staging
FIREBASE_PRIVATE_KEY=staging_firebase_private_key
FIREBASE_CLIENT_EMAIL=firebase-adminsdk@finclick-staging.iam.gserviceaccount.com

# ==============================================
# HEALTH CHECK CONFIGURATION
# ==============================================
HEALTH_CHECK_INTERVAL=30
HEALTH_CHECK_TIMEOUT=10
HEALTH_CHECK_RETRIES=3

# ==============================================
# SSL/TLS CONFIGURATION (Self-signed OK)
# ==============================================
SSL_CERT_PATH=/etc/nginx/ssl/staging.finclick.ai.crt
SSL_KEY_PATH=/etc/nginx/ssl/staging.finclick.ai.key
SSL_CA_PATH=/etc/nginx/ssl/staging.finclick.ai.ca.crt

# ==============================================
# DOCKER CONFIGURATION
# ==============================================
COMPOSE_PROJECT_NAME=finclick-staging
DOCKER_HOST_IP=127.0.0.1

# ==============================================
# TESTING CONFIGURATION
# ==============================================
TEST_USER_EMAIL=test@finclick.ai
TEST_USER_PASSWORD=test123456
ENABLE_TEST_ROUTES=true
MOCK_EXTERNAL_APIS=true

# ==============================================
# STAGING SPECIFIC
# ==============================================
STAGING_BANNER=true
STAGING_MESSAGE=This is a staging environment for testing purposes
AUTO_RESET_DATA=false
SEED_TEST_DATA=true

# ==============================================
# MAINTENANCE MODE
# ==============================================
MAINTENANCE_MODE=false
MAINTENANCE_MESSAGE=Staging environment is under maintenance

# ==============================================
# ENVIRONMENT VERIFICATION
# ==============================================
# This should always be 'staging' for staging environment
ENVIRONMENT_VERIFICATION=staging