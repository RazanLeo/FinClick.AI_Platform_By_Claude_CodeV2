# FinClick.AI Development Environment Variables
# Copy this file to .env.development and update the values

# ==============================================
# APPLICATION CONFIGURATION
# ==============================================
NODE_ENV=development
APP_VERSION=1.0.0-dev
APP_NAME=FinClick.AI Development
APP_URL=http://localhost:3000
API_URL=http://localhost:8000

# ==============================================
# DATABASE CONFIGURATION
# ==============================================

# PostgreSQL Configuration
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=finclick_dev
POSTGRES_USER=finclick_dev
POSTGRES_PASSWORD=dev123

# MongoDB Configuration
MONGO_HOST=localhost
MONGO_PORT=27017
MONGO_DB=finclick_dev
MONGO_ROOT_USER=admin
MONGO_ROOT_PASSWORD=dev123

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0

# ==============================================
# MESSAGE QUEUE CONFIGURATION
# ==============================================
RABBITMQ_HOST=localhost
RABBITMQ_PORT=5672
RABBITMQ_USER=guest
RABBITMQ_PASSWORD=guest
RABBITMQ_VHOST=/
RABBITMQ_URL=amqp://guest:guest@localhost:5672/

# ==============================================
# SECURITY CONFIGURATION
# ==============================================

# JWT Configuration
JWT_SECRET_KEY=dev_jwt_secret_key_not_for_production
JWT_EXPIRES_IN=24h
JWT_REFRESH_EXPIRES_IN=7d

# Encryption Keys
ENCRYPTION_KEY=dev_32_character_encryption_key12
HASH_SALT_ROUNDS=4

# Session Configuration
SESSION_SECRET=dev_session_secret_key
SESSION_TIMEOUT=3600

# ==============================================
# AI SERVICE CONFIGURATION (Development)
# ==============================================

# OpenAI Configuration (Development)
OPENAI_API_KEY=sk-dev-openai-key-placeholder
OPENAI_MODEL=gpt-3.5-turbo
OPENAI_MAX_TOKENS=1000
OPENAI_TEMPERATURE=0.7

# Google Gemini Configuration (Development)
GEMINI_API_KEY=dev-gemini-api-key

# Anthropic Claude Configuration (Development)
ANTHROPIC_API_KEY=dev-anthropic-api-key

# ==============================================
# FINANCIAL DATA PROVIDERS (Development)
# ==============================================

# Financial Modeling Prep (Demo)
FMP_API_KEY=demo
FMP_BASE_URL=https://financialmodelingprep.com/api/v3

# Alpha Vantage (Demo)
ALPHA_VANTAGE_API_KEY=demo

# Quandl (Demo)
QUANDL_API_KEY=demo

# ==============================================
# PAYMENT PROCESSING (Test Mode)
# ==============================================

# Stripe Configuration (Test)
STRIPE_PUBLISHABLE_KEY=pk_test_dev_stripe_key
STRIPE_SECRET_KEY=sk_test_dev_stripe_secret
STRIPE_WEBHOOK_SECRET=whsec_test_dev_webhook

# PayPal Configuration (Sandbox)
PAYPAL_MODE=sandbox
PAYPAL_CLIENT_ID=dev_paypal_client_id
PAYPAL_CLIENT_SECRET=dev_paypal_secret

# ==============================================
# EMAIL CONFIGURATION (Development)
# ==============================================

# SMTP Configuration (MailHog for development)
SMTP_HOST=localhost
SMTP_PORT=1025
SMTP_SECURE=false
SMTP_USERNAME=
SMTP_PASSWORD=

# Email Templates
FROM_EMAIL=dev@finclick.ai
FROM_NAME=FinClick.AI Development
SUPPORT_EMAIL=dev-support@finclick.ai

# ==============================================
# AWS CONFIGURATION (Development)
# ==============================================

# AWS General (LocalStack or test credentials)
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=test
AWS_SECRET_ACCESS_KEY=test
AWS_ENDPOINT_URL=http://localhost:4566

# S3 Configuration (LocalStack)
AWS_S3_BUCKET=finclick-dev-files
AWS_S3_REGION=us-east-1
AWS_BACKUP_BUCKET=finclick-dev-backups
USE_LOCAL_STORAGE=true

# ==============================================
# LOGGING AND MONITORING
# ==============================================

# Log Configuration
LOG_LEVEL=debug
LOG_FORMAT=simple
LOG_MAX_FILES=3
LOG_MAX_SIZE=1m

# Sentry Configuration (Development - Optional)
SENTRY_DSN=
SENTRY_ENVIRONMENT=development
SENTRY_RELEASE=1.0.0-dev

# ==============================================
# GRAFANA AND PROMETHEUS
# ==============================================

# Grafana Configuration
GRAFANA_ADMIN_PASSWORD=dev123
GRAFANA_SECRET_KEY=dev_grafana_secret_key

# Prometheus Configuration
PROMETHEUS_RETENTION_TIME=7d
PROMETHEUS_RETENTION_SIZE=1GB

# ==============================================
# ELASTICSEARCH CONFIGURATION
# ==============================================
ELASTIC_USERNAME=elastic
ELASTIC_PASSWORD=dev123
ELASTICSEARCH_URL=http://localhost:9200

# ==============================================
# RATE LIMITING (Very Relaxed)
# ==============================================
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=10000
RATE_LIMIT_SKIP_FAILED_REQUESTS=true

# ==============================================
# CORS CONFIGURATION (Permissive)
# ==============================================
CORS_ORIGIN=*
CORS_CREDENTIALS=true

# ==============================================
# FILE UPLOAD CONFIGURATION
# ==============================================
MAX_FILE_SIZE=10485760
ALLOWED_FILE_TYPES=pdf,doc,docx,xls,xlsx,csv,txt,png,jpg,jpeg,gif
UPLOAD_PATH=/app/uploads

# ==============================================
# CACHE CONFIGURATION
# ==============================================
CACHE_TTL=300
CACHE_PREFIX=dev:finclick:
CACHE_MAX_KEYS=1000

# ==============================================
# NOTIFICATION CONFIGURATION
# ==============================================

# Slack Notifications (Development Webhook)
SLACK_WEBHOOK_URL=
SLACK_CHANNEL=#dev-alerts

# SMS Configuration (Test)
TWILIO_ACCOUNT_SID=
TWILIO_AUTH_TOKEN=
TWILIO_PHONE_NUMBER=

# ==============================================
# ANALYTICS CONFIGURATION
# ==============================================

# Google Analytics (Development Property)
GA_TRACKING_ID=

# Mixpanel (Development Project)
MIXPANEL_TOKEN=

# ==============================================
# BACKUP CONFIGURATION
# ==============================================
BACKUP_SCHEDULE=0 4 * * *
BACKUP_RETENTION_DAYS=3
BACKUP_ENCRYPTION_KEY=dev_backup_encryption_key

# ==============================================
# PERFORMANCE CONFIGURATION
# ==============================================
NODE_OPTIONS=--max-old-space-size=512
UV_THREADPOOL_SIZE=32

# ==============================================
# FEATURE FLAGS (All Enabled)
# ==============================================
FEATURE_AI_RECOMMENDATIONS=true
FEATURE_ADVANCED_ANALYTICS=true
FEATURE_REAL_TIME_UPDATES=true
FEATURE_BETA_FEATURES=true
FEATURE_DEBUG_MODE=true
FEATURE_HOT_RELOAD=true

# ==============================================
# DEVELOPMENT SPECIFIC
# ==============================================

# Debug Configuration
DEBUG=finclick:*
NODE_DEBUG=*
VERBOSE_LOGGING=true

# Hot Reload
FAST_REFRESH=true
CHOKIDAR_USEPOLLING=true
WATCHPACK_POLLING=true

# Development Tools
ENABLE_DEV_TOOLS=true
ENABLE_REDUX_DEVTOOLS=true
ENABLE_REACT_DEVTOOLS=true

# Mock Configuration
MOCK_AI_RESPONSES=true
MOCK_PAYMENT_PROCESSING=true
MOCK_EMAIL_SENDING=true
MOCK_EXTERNAL_APIS=true

# ==============================================
# THIRD-PARTY INTEGRATIONS (Development)
# ==============================================

# Supabase (Development)
SUPABASE_URL=http://localhost:54321
SUPABASE_ANON_KEY=dev_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=dev_supabase_service_key

# Firebase (Development)
FIREBASE_PROJECT_ID=finclick-dev
FIREBASE_PRIVATE_KEY=dev_firebase_private_key
FIREBASE_CLIENT_EMAIL=firebase-adminsdk@finclick-dev.iam.gserviceaccount.com

# ==============================================
# HEALTH CHECK CONFIGURATION
# ==============================================
HEALTH_CHECK_INTERVAL=10
HEALTH_CHECK_TIMEOUT=5
HEALTH_CHECK_RETRIES=3

# ==============================================
# SSL/TLS CONFIGURATION (Not required for dev)
# ==============================================
SSL_ENABLED=false

# ==============================================
# DOCKER CONFIGURATION
# ==============================================
COMPOSE_PROJECT_NAME=finclick-dev
DOCKER_HOST_IP=127.0.0.1

# ==============================================
# TESTING CONFIGURATION
# ==============================================
TEST_USER_EMAIL=test@finclick.ai
TEST_USER_PASSWORD=test123
ENABLE_TEST_ROUTES=true
AUTO_SEED_DATA=true
RESET_DB_ON_START=false

# ==============================================
# DATABASE SEEDING
# ==============================================
SEED_USERS=true
SEED_FINANCIAL_DATA=true
SEED_TEST_PORTFOLIOS=true
SEED_SAMPLE_REPORTS=true

# ==============================================
# DEVELOPMENT SERVER CONFIGURATION
# ==============================================
WEBPACK_DEV_SERVER_HOST=0.0.0.0
WEBPACK_DEV_SERVER_PORT=3000
API_DEV_SERVER_PORT=8000

# ==============================================
# MAINTENANCE MODE
# ==============================================
MAINTENANCE_MODE=false
MAINTENANCE_MESSAGE=Development environment is under maintenance

# ==============================================
# ENVIRONMENT VERIFICATION
# ==============================================
# This should always be 'development' for development environment
ENVIRONMENT_VERIFICATION=development